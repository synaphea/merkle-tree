{
  "name": "@synaphea/merkle-tree",
  "version": "1.0.2",
  "description": "Our merkle tree implementation",
  "main": "index.js",
  "keywords": [
    "merkle",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theofilis/merkle-tree.git"
  },
  "scripts": {
    "compile": "node-gyp rebuild",
    "build": "./node_modules/.bin/webpack",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --timeout 5000 -b -r ts-node/register test/**/*.spec.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "George Theofilis",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/crypto-js": "^3.1.37",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.24",
    "@types/webpack": "^3.0.10",
    "awesome-typescript-loader": "^3.2.3",
    "ajv": "^5.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.1",
    "crypto-js": "^3.1.9-1",
    "mocha": "^3.5.0",
    "nan": "^2.7.0",
    "node-gyp": "^3.6.2",
    "node-loader": "^0.6.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "nyc": "^11.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-cleanup-plugin": "^0.5.1"
  }
}
