{
  "name": "@synaphea/merkle-tree",
  "version": "1.0.19",
  "description": "Our merkle tree implementation",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "merkle",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theofilis/merkle-tree.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --reporter mocha-circleci-reporter --slow 0.0001 --timeout 10000 -b -r ts-node/register -P tsconfig.test.json test/**/*.spec.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "George Theofilis",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/crypto-js": "3.1.37",
    "@types/mocha": "2.2.42",
    "@types/node": "8.0.24",
    "chai": "4.1.1",
    "mocha": "3.5.0",
    "mocha-circleci-reporter": "0.0.2",
    "nyc": "11.1.0",
    "ts-node": "3.3.0",
    "tslint": "5.6.0",
    "tslint-config-standard": "6.0.1",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "@synaphea/mhash": "1.0.0"
  }
}
